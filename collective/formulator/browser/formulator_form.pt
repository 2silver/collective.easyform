<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      i18n:domain="collective.formulator"
      tal:omit-tag="">
  <metal:form metal:use-macro="context/@@ploneform-macros/form">
    <metal:top-slot metal:fill-slot="formtop">
      <div id="formPrologue"
           tal:condition="context/formPrologue"
           tal:content="structure context/formPrologue/output" />
    </metal:top-slot>
    <metal:top-slot metal:fill-slot="formbottom">
      <div id="formEpilogue"
           tal:condition="context/formEpilogue"
           tal:content="structure context/formEpilogue/output" />
    </metal:top-slot>
  </metal:form>
</html>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      i18n:domain="collective.formulator"
      tal:omit-tag="">
  <metal:form metal:use-macro="context/@@ploneform-macros/titlelessform">
    <metal:top-slot metal:fill-slot="formtop">
      <div id="formPrologue"
           tal:condition="view/prologue"
           tal:content="structure view/prologue/output" />
    </metal:top-slot>
    <metal:top-slot metal:fill-slot="formbottom">
      <div id="formEpilogue"
           tal:condition="view/epilogue"
           tal:content="structure view/epilogue/output" />
    </metal:top-slot>
  </metal:form>
</html>
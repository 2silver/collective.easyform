<html metal:use-macro="context/main_template/macros/master"
      i18n:domain="collective.formulator">
<body>
    <metal:block fill-slot="main">
        <h1 class="documentFirstHeading" tal:content="view/label | nothing" />
        <div id="content-core">
<b tal:content="view/form_instance/mode|nothing" />
in
            <div id="form-output" tal:condition="view/form_instance/output | nothing">
                <b tal:content="view/form_instance/output" />
            </div>
out
            <span tal:replace="structure view/contents" />
        </div>
    </metal:block>
</body>
</html>